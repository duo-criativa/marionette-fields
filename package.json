  "bugs": {
    "url": "https://github.com/duo-criativa/marionette-fields/issues"
  },
  "homepage": "https://github.com/duo-criativa/marionette-fields#readme",
  "devDependencies": {
    "backbone": "^1.3.3",
    "jest": "^23.1.0",
    "jquery": "^3.3.1"
  },
  "dependencies": {
    "ampersand-state": "^5.0.3",
    "backbone.marionette": "^3.5.1",
